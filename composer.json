{
    "name": "tsz/contao-lost-and-found-animals",
    "type": "contao-bundle",
    "description": "Lost and Found Animals extension for Contao Open Source CMS",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Andreas Samitsch",
            "homepage": "http://tiersuchzentrale.at"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "contao/core-bundle": "4.6.*",
        "symfony/framework-bundle": "^4.1"
    },
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "friendsofphp/php-cs-fixer": "^2.12",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message-factory": "^1.0.2",
        "phpunit/phpunit": "^5.7.26",
        "symfony/phpunit-bridge": "^3.2"
    },
    "extra": {
        "contao-manager-plugin": "Tsz\\ContaoLostAndFoundAnimals\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Tsz\\ContaoLostAndFoundAnimals\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tsz\\ContaoLostAndFoundAnimals\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "http://tiersuchzentrale.at",
        "source": "http://tiersuchzentrale.at"
    }
}
